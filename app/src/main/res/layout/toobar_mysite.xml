<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="@dimen/dimens_heigth_toobar"
    android:background="@drawable/custom_background_toobar"
    >
    <ImageView
        android:id="@+id/img_back_MysiteToobar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/ic_keyboard_arrow_left_black_24dp"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/dimens_margin"
        />
    <TextView
        android:gravity="center"
        android:text="@string/Str_txtMySite"
        android:textSize="@dimen/dimens_textsize"
        android:textColor="@color/color_txtWhite"
        android:layout_toRightOf="@id/img_back_MysiteToobar"
        android:layout_toLeftOf="@id/img_find_MysiteToobar"
        android:id="@+id/txt_name_MySiteToobar"
        android:textStyle="bold"
        android:layout_centerInParent="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/img_find_MysiteToobar"
        android:src="@drawable/ic_search_black_24dp"
        android:layout_centerVertical="true"
        android:layout_marginRight="@dimen/dimens_margin"
        android:layout_alignParentRight="true"
        />
</RelativeLayout>